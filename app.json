{
  "env": {
    "USE_GOOGLE_ADS_CONFIG": {
      "description": "Would you like to use a google-ads.yaml (Y) - please upload one, otherewise (N) you'll be asked to enter credentials later",
      "required": true,
      "value": ""
    }
  },
  "options": {
    "allow-unauthenticated": true,
    "port": 7681
  },
  "build": {
    "skip": false
  },
  "hooks": {
    "prebuild": {
      "commands": [
        "./cloud-run-hooks/prebuild.sh"
      ]
    },
    "postbuild": {
      "commands": [
        "echo 'postbuild'"
      ]
    },
    "precreate": {
      "commands": [
        "echo 'precreate'"
      ]
    },
    "postcreate": {
      "commands": [
        "./cloud-run-hooks/postcreate.sh"
      ]
    }
  }
}