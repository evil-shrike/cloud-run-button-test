{
  "options": {
    "allow-unauthenticated": true,
    "port": 7681
  },
  "build": {
    "skip": false
  },
  "hooks": {
    "prebuild": {
      "commands": [
        "./prebuild.sh"
      ]
    },
    "postbuild": {
      "commands": [
        "echo 'postbuild'"
      ]
    },
    "precreate": {
      "commands": [
        "echo 'precreate'"
      ]
    },
    "postcreate": {
      "commands": [
        "./postcreate.sh"
      ]
    }
  }
}