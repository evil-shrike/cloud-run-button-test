{
  "options": {
    "allow-unauthenticated": true,
    "port": 7681
  },
  "build": {
    "skip": false
  },
  "hooks": {
    "prebuild": {
      "commands": [
        "./cloud-run-hooks/prebuild.sh"
      ]
    },
    "postbuild": {
      "commands": [
        "echo 'postbuild'"
      ]
    },
    "postcreate": {
      "commands": [
        "chmod +x ./cloud-run-hooks/postcreate.sh",
        "./cloud-run-hooks/postcreate.sh"
      ]
    }
  }
}